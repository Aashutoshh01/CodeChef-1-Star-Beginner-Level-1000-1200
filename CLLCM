#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    int flag=1;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        if(a[i]%2==0){
	            flag=0;
	            break;
	        }
	    }
	    if(flag==1){
	        cout<<"YES"<<endl;
	    }
	    else if(flag==0){
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
