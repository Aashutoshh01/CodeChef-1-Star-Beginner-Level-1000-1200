#include <iostream>
#include <math.h>
using namespace std;

int main() {
	// your code goes here
	long int t;
	cin>>t;
	while(t--){
	    long int n;
	    cin>>n;
	    if(n==1 || n==2){
	        cout<<"1"<<endl;
	    }
	    else if(n==3){
	        cout<<"2"<<endl;
	    }
	    else{
	        cout<<n<<endl;
	    }
	}
	return 0;
}
