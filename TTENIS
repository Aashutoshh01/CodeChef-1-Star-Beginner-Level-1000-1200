#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    int a=0,b=0;
	    for(int i=0;s[i]!='\0';i++){
	        if(s[i]=='0'){
	            a++;
	        }
	        else if(s[i]=='1'){
	            b++;
	        }
	    }
	    if(a>b){
	        cout<<"LOSE"<<endl;
	    }
	    else if(b>a){
	        cout<<"WIN"<<endl;
	    }
	}
	return 0;
}
