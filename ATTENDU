#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string b;
	    cin>>b;
	    int a=0;
	    for(int i=0;b[i]!='\0';i++){
	        if(b[i]=='0'){
	            a++;
	        }
	    }
	    if(a>30){
	        cout<<"NO"<<endl;
	    }
	    else{
	        cout<<"YES"<<endl;
	    }
	}
	return 0;
}
