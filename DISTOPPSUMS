#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int p[n];
	    int count=1;
	    for(int i=0;i<n/2;i++){
	        p[i]=count++;
	        p[n-i-1]=count++;
	    }
	    for(int i=0;i<n;i++){
	        cout<<p[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
