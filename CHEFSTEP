#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long int n,k;
	    cin>>n>>k;
	    long int d[n];
	    for(long int i=0;i<n;i++){
	        cin>>d[i];
	    }
	    for(long int i=0;i<n;i++){
	        if(d[i]%k==0){
	            cout<<"1";
	        }
	        else{
	            cout<<"0";
	        }
	    }
	    cout<<endl;
	}
	return 0;
}
