#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long int n;
	cin>>n;
	long int c[n];
	long int sum=0;
	for(long int i=0;i<n;i++){
	    cin>>c[i];
	}
	for(long int i=0;i<n;i++){
	    sum+=c[i];
	}
	if(sum==n*(n+1)/2){
	    cout<<"YES"<<endl;
	}
	else{
	    cout<<"NO"<<endl;
	}
	return 0;
}
