#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,y,k;
	    cin>>x>>y>>k;
	    if((x+y)%k==0){
	        if((x+y)%2==0){
	            cout<<"Chef"<<endl;
	        }
	        else{
	            cout<<"Paja"<<endl;
	        }
	    }
	    else{
	        if(((x+y)/k)%2==0){
	            cout<<"Chef"<<endl;
	        }
	        else{
	            cout<<"Paja"<<endl;
	        }
	    }
	}
	return 0;
}
