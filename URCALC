#include <iostream>
using namespace std;

int main() {
	// your code goes here
	double a,b;
	char o;
	cin>>a>>b>>o;
	    if(o=='+'){
	        cout<<a+b;
	    }
	    else if(o=='-'){
	        cout<<a-b;
	    }
	    else if(o=='*'){
	        cout<<a*b;
	    }
	    else if(o=='/'){
	        cout<<a/b;
	    }
	return 0;
}
