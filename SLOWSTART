#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,h;
	    cin>>x>>h;
	    if(h<=5*x/2){
	        if(h%(x/2)==0){
	            cout<<h/(x/2)<<endl;
	        }
	        else{
	            cout<<(h/(x/2))+1<<endl;
	        }
	    }
	    else{
	        if((h-5*x/2)%(x)==0){
	            cout<<5+(h-5*x/2)/(x)<<endl;
	        }
	        else{
	            cout<<5+(h-5*x/2)/(x)+1<<endl;
	        }
	    }
	}
	return 0;
}
