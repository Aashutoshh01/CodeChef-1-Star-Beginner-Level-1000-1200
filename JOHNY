#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,k;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    cin>>k;
	    int flag=a[k-1];
	    int count=0;
	    for(int i=0;i<n;i++){
	        if(a[i]<flag){
	            count++;
	        }
	    }
	    cout<<1+count<<endl;
	}
	return 0;
}
