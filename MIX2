#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long int n;
	    cin>>n;
	    long int a[n];
	    long int flag=0;
	    for(long int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n-1;i++){
	        for(int j=i+1;j<n;j++){
	            flag+=a[i]*a[j];
	        }
	    }
	    cout<<flag<<endl;
	}
	return 0;
}
