#include <iostream>
#include <math.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    float a,b,c;
	    cin>>a>>b>>c;
	    if(2*(a/c)<(sqrt(2)*a)/b){
	        cout<<"Elevator"<<endl;
	    }
	    else{
	        cout<<"Stairs"<<endl;
	    }
	}
	return 0;
}
