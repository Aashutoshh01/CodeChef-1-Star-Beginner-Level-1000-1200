#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int  t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string a,b;
	    int c=0,d=0;
	    cin>>a>>b;
	    for(int i=0;i<n;i++){
	        c+=int(a[i]);
	    }
	    for(int i=0;i<n;i++){
	        d+=int(b[i]);
	    }
	    if(c==d){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
