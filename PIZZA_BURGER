#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,y,z;
	    cin>>x>>y>>z;
	    if(x<y && x<z){
	        cout<<"NOTHING"<<endl;
	    }
	    else if(x>=y && x<z){
	        cout<<"PIZZA"<<endl;
	    }
	    else if(x<y && x>=z){
	        cout<<"BURGER"<<endl;
	    }
	    else if(x>=y && x>=z){
	        cout<<"PIZZA"<<endl;
	    }
	}
	return 0;
}
