#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long int t;
	cin>>t;
	while(t--){
	    long int n;
	    cin>>n;
	    long int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int flag=0;
	    for(int i=0;i<n;i++){
	        if(a[i]%2!=0){
	            flag++;
	        }
	    }
	    if(flag%2==0 && flag!=0){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
