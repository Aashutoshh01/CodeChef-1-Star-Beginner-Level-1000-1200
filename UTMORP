#include <iostream>
#include <math.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long int n,k;
	    cin>>n>>k;
	    long int a[n];
	    int sum=0;
	    for(long int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(long int i=0;i<n;i++){
	        sum+=a[i];
	    }
	    if(k==1){
	        if((sum+1)%2==0){
	            cout<<"even"<<endl;
	        }
	        else{
	            cout<<"odd";
	        }
	    }
	    else{
	        cout<<"even"<<endl;
	    }
	}
	return 0;
}
