#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,p;
	    cin>>n>>p;
	    int r[n];
	    int a=0,b=0;
	    for(int i=0;i<n;i++){
	        cin>>r[i];
	    }
	    for(int i=0;i<n;i++){
	        if(r[i]>=p/2){
	            a++;
	        }
	        if(r[i]<=p/10){
	            b++;
	        }
	    }
	    if(a==1 && b==2){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
