#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    int sum=0;
	    for(int i=0;s[i]!='\0';i++){
	        if(int(s[i])>=48 && int(s[i])<=57){
	            sum+=int(s[i])-48;
	        }
	    }
	    cout<<sum<<endl;
	}
	return 0;
}
