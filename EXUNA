#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long int t;
	cin>>t;
	while(t--){
	    long int n;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int min=a[0];
	    for(int i=1;i<n;i++){
	        if(a[i]<min){
	            min=a[i];
	        }
	    }
	    cout<<min<<endl;
	}
	return 0;
}
